{
  "name": "backstage-plugins",
  "private": true,
  "workspaces": [
    "workspaces/*"
  ],
  "engines": {
    "node": "18 || 20"
  },
  "scripts": {
    "dev": "backstage-cli package start",
    "start": "backstage-cli package start",
    "build": "backstage-cli package build",
    "build:all": "./scripts/generate-types.sh && yarn workspaces run build",
    "lint": "backstage-cli package lint",
    "lint:fix": "backstage-cli package lint --fix",
    "lint:all": "yarn workspaces run lint",
    "test": "yarn workspaces run test",
    "test:coverage": "yarn workspaces run test --coverage",
    "test:all": "yarn workspaces run test",
    "clean": "backstage-cli package clean",
    "clean:all": "yarn workspaces run clean",
    "tsc:full": "tsc --noEmit false --emitDeclarationOnly true",
    "diff": "backstage-cli package diff",
    "prepack": "backstage-cli package prepack",
    "postpack": "backstage-cli package postpack",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "changeset publish",
    "prepare": "husky install",
    "create-plugin": "./scripts/create-plugin.sh"
  },
  "devDependencies": {
    "@backstage/cli": "^0.26.0",
    "@backstage/eslint-plugin": "^0.1.8",
    "@changesets/cli": "^2.27.1",
    "@commitlint/cli": "^18.4.0",
    "@commitlint/config-conventional": "^18.4.0",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^9.2.1",
    "eslint": "^8.57.1",
    "husky": "^8.0.3",
    "lint-staged": "^16.1.6",
    "prettier": "^3.0.0",
    "typescript": "~5.2.0"
  },
  "resolutions": {
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@azure/identity": "^3.4.2",
    "@azure/core-auth": "^1.5.0",
    "@azure/core-client": "^1.8.2",
    "@azure/core-rest-pipeline": "^1.15.2",
    "@azure/core-lro": "^2.6.2",
    "@azure/core-paging": "^1.5.2",
    "@azure/core-tracing": "^1.1.2",
    "@azure/core-util": "^1.2.0",
    "@azure/logger": "^1.0.4",
    "@azure/msal-node": "^2.9.2",
    "@azure/abort-controller": "^1.0.5",
    "@azure/core-http-compat": "^1.3.0",
    "@azure/core-xml": "^1.3.2",
    "@azure/storage-blob": "^12.17.0",
    "@azure/storage-common": "^12.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,cjs}": [
      "prettier --write"
    ],
    "*.{json,md,yaml,yml}": [
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devexcom/backstage-plugins.git"
  },
  "keywords": [
    "backstage",
    "plugins"
  ],
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
